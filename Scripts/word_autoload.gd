enum GameModes {
	Endless,
	Levels,
	Daily
}
var current_gamemode


func _ready():
	var energy_timer = Timer.new()
	get_parent().add_child.call_deferred(energy_timer)
	energy_timer.wait_time = 900
	energy_timer.autostart = true
	energy_timer.timeout.connect(gain_energy)
	current_unix = int(Time.get_unix_time_from_system())
	seed(8)
	daily_words_list = words_list.duplicate()
	daily_words_list.shuffle()
	randomize()
	current_unix = int(Time.get_unix_time_from_system())
	day = (current_unix - launch_unix) / 86400
	if last_unix != null:
		var energy_difference = (last_unix - current_unix) / 900
		if energy_difference > 0:
			energy = min(energy + energy_difference, 4)


func set_game_word(from_button : int = -1):
	match current_gamemode:
		GameModes.Levels:
			level = from_button
			game_word = words_list[from_button - 1]
		GameModes.Endless:
			game_word = words_list.pick_random()
			return game_word
		GameModes.Daily:
			game_word = daily_words_list[day]


func add_completed_level():
	completed_levels.append(level)
	SaveManager.save_data()


func set_high_score(new_high_score):
	high_score = new_high_score


func gain_energy():
	energy = min(energy + 1, 4)
	if in_menu:
		get_node("../MainMenu").change_energy_ui()
	elif in_levels:
		get_node("../Levels").change_energy_ui()


func lose_energy():
	energy -= 1


func set_gamemode(new_gamemode: GameModes):
	current_gamemode = new_gamemode
